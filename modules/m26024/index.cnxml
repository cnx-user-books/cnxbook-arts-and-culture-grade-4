          <colspec colnum="5" colname="c5"/>
          <colspec colnum="6" colname="c6"/>
          <tbody>
            <row>
              <entry>a</entry>
              <entry>himself and other composers.</entry>
              <entry>b</entry>
              <entry>animals.</entry>
              <entry>c</entry>
              <entry>people.</entry>
            </row>
          </tbody>
        </tgroup>
      </table>
